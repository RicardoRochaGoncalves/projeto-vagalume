<% for(var i = 0; i < estabelecimentos.length; i++) { %> 
<tr>
    <td> <%= estabelecimentos[i].nome %> </td>
    <td><%= estabelecimentos[i].endereco %> </td>
    <td><%=  estabelecimentos[i].categoria %> </td>
    <td>
        <a href="/update-estabelecimento?id=<%= estabelecimentos[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow deleteestabelecimento"  data-estabelecimento-id=<%= estabelecimentos[i]._id%>>
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>

<% } %> 